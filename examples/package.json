{
  "name": "tests",
  "private": true,
  "scripts": {
    "build": "METEOR_PACKAGE_DIRS=../ meteor meteor-build-client ../docs -t ../build/template.html -p \"\" && cp ../build/loadingScreen.css ../docs && chmod -R 770 ../docs",
    "examples": "METEOR_PACKAGE_DIRS=../ meteor",
    "kill": "kill -9 `ps ax | grep node | grep meteor | awk '{print $1}'`",
    "start": "meteor run",
    "test": "TEST_WATCH=1 meteor test --driver-package meteortesting:mocha",
    "test-cli": "meteor test --once --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "lint": "standard",
    "lint-fix": "standard --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "bootstrap": "^4.3.1",
    "jquery": "^3.3.1",
    "meteor-node-stubs": "^0.4.1",
    "popper.js": "^1.14.7"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    },
    "testModule": "tests/main.js"
  },
  "devDependencies": {
    "standard": "^12.0.1"
  }
}
